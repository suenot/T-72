@import '../../css/_vars'
.header
	height: 125px
	width: 100%
	background: yellow !important
	display: block
	svg path
		fill: #fff
	@media (max-width: $screen-ipad-max)
		height: 155px
		position: relative
		.logo
			svg path
				fill: black
	&__wrap
		height: 125px
		display: flex
	&__blocks
		align-self: center
		width: 100%
		height: 53px
		position: relative